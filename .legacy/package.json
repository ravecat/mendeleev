{
  "name": "mendeleev",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ravecat/mendeleev.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "camel-case": "^3.0.0",
    "connected-react-router": "^6.5.2",
    "highcharts": "^8.0.0",
    "highcharts-react-official": "^2.2.2",
    "history": "^4.10.1",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.1",
    "react-responsive": "^6.1.1",
    "react-router-dom": "^4.3.1",
    "react-router-prop-types": "^1.0.4",
    "react-scripts": "3.2.0",
    "recharts": "^2.0.0-beta.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.1.3",
    "styled-components-toolbox": "0.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/gitlab": "^4.0.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.6",
    "customize-cra": "^0.9.1",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.0.9",
    "lint-staged": "9.4.3",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "sass-extract": "^2.1.0",
    "sass-extract-js": "^0.4.0",
    "sass-extract-loader": "^1.1.0",
    "semantic-release": "^15.13.30",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-app-rewired test --passWithNoTests",
    "eject": "react-scripts eject",
    "release:debug": "semantic-release --debug",
    "lint:eslint": "eslint '**/*.{js,jsx}'",
    "lint:stylelint": "stylelint '**/*.{js,jsx,css}'",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:stylelint",
    "prettier": "prettier --check *.{css,md,yml,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "stylelint",
      "git add"
    ],
    "*.{css,md,yml,json}": [
      "prettier --write",
      "git add"
    ],
    "*": [
      "yarn test",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.config.js"
    }
  }
}
